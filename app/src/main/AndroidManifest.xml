<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.voador.guardeiro.flightclub">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fight_club"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fight_club"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.RegistrationActivity" />
        <activity android:name=".activities.AlunoActivity" />
        <activity android:name=".activities.TabletMainActivity" />
        <!--<activity android:name=".MatriculaActivity" />-->
        <activity
            android:name=".activities.ModalidadeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.PlanoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.GraduacaoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activities.MatriculaActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>